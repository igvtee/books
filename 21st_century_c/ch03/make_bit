    push:
        @if [ "x$(MSG)" = 'x' ] ; then \
                echo "Usage: MSG='your message here.' make push"; fi
        @test "x$(MSG)" != 'x'
        git commit -a  -m "$(MSG)"
        git svn fetch
        git svn rebase
        git svn dcommit

    pull:
        git svn fetch
        git svn rebase
